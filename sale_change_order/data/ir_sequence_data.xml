<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<!-- Sequences for change.order -->
		<record id="seq_change_order_sale" model="ir.sequence">
			<field name="name">Change Sales Order</field>
			<field name="code">change.order.sale</field>
			<field name="prefix">CSO</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="seq_change_order_purchase" model="ir.sequence">
			<field name="name">Change Purchase Order</field>
			<field name="code">change.order.purchase</field>
			<field name="prefix">CPO</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>
	</data>
</odoo>